{
  env: {
    "es6": true,
    "meteor": true,
    "browser": true
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-extend-native": 2,
    "block-scoped-var": 2,
    "indent": [
      2,
      2
    ],
    "quotes": [
        2,
        "double"
    ],
    "max-depth": [
      2,
      4
    ],
    "max-len": [
      2,
      80
    ],
    "new-cap": [2, {
      capIsNewExceptions: [
        "CryptoJS",
        "LoadScript",
        "MD5",
        "Match",
        "Optional"
      ],
    }],
    "no-caller": 2,
    "no-unused-vars": 2,
    "no-debugger": 2,
    "no-eq-null": 2,
    "dot-notation": 0,
    "no-new": 0
  }
}
